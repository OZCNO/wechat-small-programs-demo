
.container {
  display: flex;
  flex-direction: column;
  width: 100%;
  padding:40rpx 50rpx;
  box-sizing: border-box;
}

.head_image {
  width: 400rpx;
  height:400rpx;
  border-radius: 50%;
  margin: 70rpx auto 50rpx;
}


.button {
  width: 100%;
  color:#fff;
  background-color: #f77;
  font-size: 35rpx;
  border-radius: 10rpx;
}

.get{
  margin-top: 100rpx;
}

.submit{
  margin-top:60rpx;
}

.tip-container{
  font-size: 30rpx;
  color:#aaa;
  bottom: 50rpx;
  margin: 0 auto;
  position: absolute;
  width:100%;
  text-align: center;
}
